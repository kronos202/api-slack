    "@bull-board/express": "^6.7.10",
    "@bull-board/nestjs": "^6.7.10",
    "@nestjs/bull": "^11.0.2",
    "@types/glob": "^8.1.0",
    "ioredis": "^5.6.0",
    "minimatch": "^10.0.1"
